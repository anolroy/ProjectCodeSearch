   SQLStr1 = "SELECT COUNT(*) AS Z " & _
             "From " & _
             "(" & _
               "SELECT SUM(DS.TOTALAMOUNT) AS TOTAL, DR.DemandID " & _
               "FROM DEMANDRECORDS AS DR, DEMANDSPLITRECORDS AS DS " & _
               "WHERE DR.DEMANDID=DS.DEMANDID AND " & _
