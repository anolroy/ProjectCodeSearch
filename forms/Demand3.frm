            rstSet!OSAmount = RoundingNumber(CCur(flxSPayment.TextMatrix(iRow, 9)) - _
                              CCur(flxSPayment.TextMatrix(iRow, 10)) - _
                              CCur(IIf(flxSPayment.TextMatrix(iRow, 11) = "", 0, flxSPayment.TextMatrix(iRow, 11))), 2)
