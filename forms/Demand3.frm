               .AddNew
               .Fields.Item("TransactionID").Value = UniqueID()
               .Fields.Item("RptHeader").Value = lRpt_ID
               .Fields.Item("FundID").Value = flxSPayment.TextMatrix(iRow, 14)
               .Fields.Item("Amount").Value = flxSPayment.TextMatrix(iRow, 10)
