Private Function PossibleToDelete(szDemandId As String, ByVal adoConn As ADODB.Connection) As Boolean
   Dim adoRst As New ADODB.Recordset
   Dim szSQL As String

   szSQL = "SELECT tlbReceipt.TransactionID " & _
           "FROM tlbReceipt, DemandRecords " & _
           "WHERE (tlbReceipt.ReceiptAmount > 0 OR " & _
               "tlbReceipt.Amount <> tlbReceipt.OSAmount) And " & _
               "tlbReceipt.DemandRef = " & szDemandId & ";"
   adoRst.Open szSQL, adoConn, adOpenStatic, adLockReadOnly
'Debug.Print szSQL
   If adoRst.EOF Then
      PossibleToDelete = True
   Else
      PossibleToDelete = False
   End If

   adoRst.Close
   Set adoRst = Nothing
End Function

