Private Sub CalSubTotal(conGrid As Control, conTextTA As Control, conTextVAT As Control, conTextTotal As Control)
   Dim iRow As Integer

   conTextTA.text = ""
   conTextVAT.text = ""
   conTextTotal.text = ""
   For iRow = 1 To conGrid.Rows - 1
      If conGrid.TextMatrix(iRow, 0) <> "DELETED" Then 'condition added by anol 20 Nov 2015
            conTextTA.text = Format(Val(conTextTA.text) + Val(conGrid.TextMatrix(iRow, 7)), "0.00")
            conTextVAT.text = Format(Val(conTextVAT.text) + Val(conGrid.TextMatrix(iRow, 8)), "0.00")
