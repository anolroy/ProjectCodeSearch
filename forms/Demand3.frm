             "SET DemandRecords.DemandHistory=TRUE " & _
             "WHERE DemandRecords.IsPrinted = TRUE AND " & _
                  "DemandRecords.DemandID IN (" & szDmdID & ");"

   adoConn.Execute SQLStr1
