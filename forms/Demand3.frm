            .BoolReconciled = True
        End If
        
        If flxReceiptHistory.TextMatrix(flxReceiptHistory.row, 13) <> "" Then
           .txtClient.text = flxReceiptHistory.TextMatrix(flxReceiptHistory.row, 13)
