             "FROM DemandRecords " & _
             "WHERE DemandRecords.DemandHistory = FALSE AND " & _
                  "DemandRecords.DemandID IN (" & szDmdID & ");"
   adoRst.Open SQLStr1, adoConn, adOpenStatic, adLockReadOnly
