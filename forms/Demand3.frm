            flxSPayment.TextMatrix(iRow, 10) = "0.00"
            flxSPayment.TextMatrix(iRow, 15) = ""
            flxSPayment.TextMatrix(iRow, 16) = ""

            If flxSPayment.TextMatrix(iRow, 0) = "+" Or _
                   flxSPayment.TextMatrix(iRow, 0) = ">" Then
               iRowSp = iRow + 1
