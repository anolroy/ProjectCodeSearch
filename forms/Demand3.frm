Private Function BankAndBalance(adoConn As ADODB.Connection) As String
   ' Error Handler
   On Error GoTo Error_Handler

   Dim iRec As Integer
   Dim adoRst As New ADODB.Recordset
   Dim szSQL As String, szaData() As String
'Modified by anol 09 Apr 2015
'added allowOverDraft, OverDraftLimit to implement bank overdraft
   If txtRptClientList.text = "ALL" Then
      szSQL = "SELECT tlbClientBanks.NominalCode AS BNC, " & _
                  "NominalLedger.Name AS BNN, tlbClientBanks.CurrentBalance AS BAL, AllowOverDraft, OverDraftLimit  " & _
              "FROM tlbClientBanks, NominalLedger " & _
              "WHERE tlbClientBanks.NominalCode = NominalLedger.Code " & _
              "GROUP BY tlbClientBanks.NominalCode, NominalLedger.Name, tlbClientBanks.CurrentBalance, AllowOverDraft, OverDraftLimit;"
   Else
      szSQL = "SELECT tlbClientBanks.NominalCode AS BNC, " & _
                  "NominalLedger.Name AS BNN, tlbClientBanks.CurrentBalance AS BAL, AllowOverDraft, OverDraftLimit  " & _
              "FROM tlbClientBanks, NominalLedger " & _
              "WHERE tlbClientBanks.NominalCode = NominalLedger.Code AND " & _
                  "tlbClientBanks.CLIENT_ID = '" & txtRptClientList.text & "' AND " & _
                  "NominalLedger.ClientID = '" & txtRptClientList.text & "' " & _
              "GROUP BY tlbClientBanks.NominalCode, NominalLedger.Name, tlbClientBanks.CurrentBalance, AllowOverDraft, OverDraftLimit;"
   End If
'Debug.Print szSQL
   adoRst.Open szSQL, adoConn, adOpenStatic, adLockReadOnly

   If adoRst.EOF Then
      MsgBox "Please setup bank account for the client."
   Else
      ReDim szaData(3, adoRst.RecordCount - 1) As String

      While Not adoRst.EOF
         szaData(0, iRec) = adoRst.Fields.Item("BNC").Value
         szaData(1, iRec) = adoRst.Fields.Item("BNN").Value
         'Modified by anol 09 Apr 2015
         szaData(2, iRec) = IIf(IsNull(adoRst.Fields.Item("AllowOverDraft").Value) = True, 0, adoRst.Fields.Item("AllowOverDraft").Value)
         szaData(3, iRec) = IIf(IsNull(adoRst.Fields.Item("OverDraftLimit").Value) = True, 0, adoRst.Fields.Item("OverDraftLimit").Value)
         'End of addition
          iRec = iRec + 1
         'BankAndBalance = BankAndBalance & CStr(adoRst.Fields.Item("BAL").Value) & " # "
         adoRst.MoveNext
      Wend
   End If

   cmbBankAc.Clear
   cmbBankAc.Column() = szaData()

   ' Destroy Objects
   Set adoRst = Nothing

   Exit Function

   ' Error Handling Code
Error_Handler:
   ' Destroy Objects
   Set adoRst = Nothing
   ShowMsgInTaskBar ERR.description, "Y", "P"
End Function

